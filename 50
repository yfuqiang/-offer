class Solution {
public:
    char firstUniqChar(string s) {
        if(s=="")
        return ' ';
        unordered_map<char,int> m;
        int i;
        for(i=0;i<s.size();i++)
        {
            m[s[i]]++;
        }
        for(i=0;i<s.size();i++)
        {
            if(m[s[i]]==1)
            return s[i];
        }
        return ' ';
    }
};
